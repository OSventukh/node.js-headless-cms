{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": "off",
        "import/extensions": ["off", "never"],
        "object-curly-newline": "off",
        "no-unused-vars": "warn",
        "object-shorthand": "warn"
    }
}
